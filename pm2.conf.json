{
  "app": "chat",
  "script": "./bin/www",
  "error_file": "./logs/err.log",
  "out_file": "./logs/out.log",
  "log_date_format": "YYYY-MM-DD HH:mm:ss",
  "merge_logs": true,
  "watch": true,
  "ignore_watch": [
    "node_modules",
    "logs"
  ],
  "instances": "1",
  "env": {
    "NODE_ENV": "production"
  }
}